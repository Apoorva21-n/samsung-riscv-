#include <stdio.h>

// Function to calculate factorial
int factorial(int n) {
    if (n <= 1) 
    return 1;
    return n * factorial(n - 1);
}

int main() {
    int num = 5; // Change this value to calculate factorial of other numbers
    printf("Factorial of %d is %d\n", num, factorial(num));
    return 0;
}

